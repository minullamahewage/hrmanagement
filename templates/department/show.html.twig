{% extends 'base.html.twig' %}

{% block title %}Department{% endblock %}

{% block body %}
    <h1>Department</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>DeptId</th>
                <td>{{ department.deptId }}</td>
            </tr>
            <tr>
                <th>DeptName</th>
                <td>{{ department.deptName }}</td>
            </tr>
            <tr>
                <th>Building</th>
                <td>{{ department.building }}</td>
            </tr>
            <tr>
                <th>Floor</th>
                <td>{{ department.floor }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('department_index') }}">back to list</a>

    <a href="{{ path('department_edit', {'deptId': department.deptId}) }}">edit</a>

    {{ include('department/_delete_form.html.twig') }}
{% endblock %}
