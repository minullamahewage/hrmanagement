<form method="post" action="{{ path('emp_data_delete', {'attribute': emp_datum.attribute}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ emp_datum.attribute) }}">
    <button class="btn">Delete</button>
</form>
