<form method="post" action="{{ path('employ_history_delete', {'empHistoryId': employ_history.empHistoryId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ employ_history.empHistoryId) }}">
    <button class="btn">Delete</button>
</form>
