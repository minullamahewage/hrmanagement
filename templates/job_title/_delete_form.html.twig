<form method="post" action="{{ path('job_title_delete', {'jobTitleId': job_title.jobTitleId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ job_title.jobTitleId) }}">
    <button class="btn">Delete</button>
</form>
