{% extends 'base.html.twig' %}

{% block title %}JobTitle index{% endblock %}

{% block body %}
    <h1>JobTitle index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>JobTitleId</th>
                <th>JobTitle</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for job_title in job_titles %}
            <tr>
                <td>{{ job_title.jobTitleId }}</td>
                <td>{{ job_title.jobTitle }}</td>
                <td>{{ job_title.description }}</td>
                <td>
                    <a href="{{ path('job_title_show', {'jobTitleId': job_title.jobTitleId}) }}">show</a>
                    <a href="{{ path('job_title_edit', {'jobTitleId': job_title.jobTitleId}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('job_title_new') }}">Create new</a>
{% endblock %}
