{% extends 'base.html.twig' %}

{% block title %}PayGrade index{% endblock %}

{% block body %}
    <h1>PayGrade index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>PayGrade</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pay_grade in pay_grades %}
            <tr>
                <td>{{ pay_grade.payGrade }}</td>
                <td>
                    <a href="{{ path('pay_grade_show', {'payGrade': pay_grade.payGrade}) }}">show</a>
                    <a href="{{ path('pay_grade_edit', {'payGrade': pay_grade.payGrade}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('pay_grade_new') }}">Create new</a>
{% endblock %}
